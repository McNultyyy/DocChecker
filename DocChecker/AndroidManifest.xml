<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.duncanwestland.chk"
      android:versionCode="7"
      android:versionName="@string/version">
    <uses-sdk android:minSdkVersion="15" android:targetSdkVersion="15"/>
    <uses-permission android:name="android.permission.NFC"></uses-permission>
    <uses-feature android:name="android.hardware.nfc" android:required="true" />

    <application android:icon="@drawable/epassport" android:label="@string/app_name" android:debuggable="false" android:hardwareAccelerated="true">
        <activity android:name="ReaderStartUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name="com.duncanwestland.chk.PassportSummaryActivity"></activity>


    </application>
</manifest>